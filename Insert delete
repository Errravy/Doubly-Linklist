 public static void Main()
    {
        int i = 0;
        LinkedList list = new LinkedList();
        Console.Write("Masukan angka : ");
        int x =Convert.ToInt32(Console.ReadLine());
        while (i != x)
        {
            list.Add(i + 1);  
            i++;
        }
        Console.Write("Daftar Linked List :");
        list.Print();
        list.InsertAtIndex(0, 50);
        Console.WriteLine("\nSetelah Insert di Head");
        list.Print();
        list.InsertAtIndex(x, 20);
        Console.WriteLine("\nSetelah Insert di Tail");
        list.Print();
        Console.Write("\nMasukan index yang akan di insert: ");
        int p = Convert.ToInt32(Console.ReadLine());
        list.InsertAtIndex(p + 1, 100);
        Console.WriteLine("\nSetelah Insert di Index ke n");
        list.Print();
        list.DeleteAtIndex(0);
        Console.WriteLine("\nSetelah Delete Head");
        list.Print();
        list.DeleteAtIndex(x + 1);
        Console.WriteLine("\nSetelah Delete Tail");
        list.Print();
        Console.Write("\nMasukan index yang akan di delete: ");
        int n = Convert.ToInt32(Console.ReadLine());
        list.DeleteAtIndex(n);
        Console.WriteLine("\nSetelah Delete di Index ke n");
        list.Print();
        Console.ReadLine();
    }
} 
