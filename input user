using System;
 class node
{
    public int data;
    public node next;
    public node prev;
     public node(int data)
     {
         this.data = data;
         this.prev = null;
         this.next = null;
     }
}
 class linklist
{
   public node head;
    public linklist()
    {
        head = null;
    }
    public int addNode(int Data)
    {
       node x = new node(Data);
       if (head == null)
       {
           head = x; 
       }
       else if (x == null)
       {
           node temp = head;
           while(temp.next != null)
           {
               temp = temp.next;
           }
           temp.next = x;
           x.prev    = temp;
           
       }
       else{}
       return x.data;
    }
    public void Head(int data)
    {
        node x = new node(data);
        if(head == null)
        {
            head = x;
        }
        else
        {
            x.next = head;
            head.prev = x;
            head = x;
            Console.Write("Head berubah : " + head.data + " ");
        }
    }
    public void mid(node temp,int afternode,int data)
    {
        if (temp == null)
        {
            return;
        }
        if(temp.data == afternode)
        {
            node x = new node(data);
           if (temp.next != null)
           {
            temp.next = x;
            temp.prev = x;
            x.next    = temp.next;
           }
            temp.next = x;
            x.prev    = temp;
        }
        else{return;}
        
    }
    public void Tail(int data)
    {
        node belakang = new node(data);
        node last = head;
        belakang.next = null;
        if (head == null)
        {
            belakang.prev = null;
            head = belakang;
            return;
            while (last.next != null)
            last = last.next;
        }
        last.next = belakang;
        belakang.prev = last;
        Console.Write(head.data + " ");
    }
    static  void Main()
    {
        int i = 0;
        linklist list = new linklist();
        node hasil = new node(i);
        Console.Write("Masukan angka : ");
       int input = Convert.ToInt32(Console.ReadLine());
       while ( i != input)
       {
           i++;
           hasil.data = list.addNode(i);
           Console.Write(hasil.data + " ");
       }
       Console.WriteLine();
       list.Head(50);
       i = 0;
       while ( i != input)
       {
           i++;
           hasil.data = list.addNode(i);
           
           Console.Write(hasil.data + " ");
       }
       Console.ReadKey();
    }
}
